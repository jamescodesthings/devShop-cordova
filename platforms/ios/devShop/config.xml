<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.secretgeek.devShop" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <name>devShop</name>
    <description>
        The devShop app by https://github.com/secretGeek
    </description>
    <author href="https://github.com/secretGeek">
        secretGeek
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="cordova/res/icon/ios/icon-60@3x.png" width="180" />
    <icon height="60" src="cordova/res/icon/ios/icon-60.png" width="60" />
    <icon height="120" src="cordova/res/icon/ios/icon-60@2x.png" width="120" />
    <icon height="76" src="cordova/res/icon/ios/icon-76.png" width="76" />
    <icon height="152" src="cordova/res/icon/ios/icon-76@2x.png" width="152" />
    <icon height="40" src="cordova/res/icon/ios/icon-small-40.png" width="40" />
    <icon height="80" src="cordova/res/icon/ios/icon-small-40@2x.png" width="80" />
    <icon height="57" src="cordova/res/icon/ios/icon.png" width="57" />
    <icon height="114" src="cordova/res/icon/ios/icon@2x.png" width="114" />
    <icon height="72" src="cordova/res/icon/ios/icon-72.png" width="72" />
    <icon height="144" src="cordova/res/icon/ios/icon-72@2x.png" width="144" />
    <icon height="167" src="cordova/res/icon/ios/icon-167.png" width="167" />
    <icon height="29" src="cordova/res/icon/ios/icon-small.png" width="29" />
    <icon height="58" src="cordova/res/icon/ios/icon-small@2x.png" width="58" />
    <icon height="87" src="cordova/res/icon/ios/icon-small@3x.png" width="87" />
    <icon height="50" src="cordova/res/icon/ios/icon-small-50.png" width="50" />
    <icon height="100" src="cordova/res/icon/ios/icon-small-50@2x.png" width="100" />
    <icon height="167" src="cordova/res/icon/ios/icon-83.5@2x.png" width="167" />
    <splash src="cordova/res/screen/ios/Default@2x~iphone~anyany.png" />
    <splash src="cordova/res/screen/ios/Default@2x~iphone~comany.png" />
    <splash src="cordova/res/screen/ios/Default@3x~iphone~anyany.png" />
    <splash src="cordova/res/screen/ios/Default@3x~iphone~comany.png" />
    <splash src="cordova/res/screen/ios/Default@2x~ipad~anyany.png" />
    <splash src="cordova/res/screen/ios/Default@2x~ipad~comany.png" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <allow-navigation href="ionic://*" />
    <preference name="deployment-target" value="11.0" />
    <feature name="IonicWebView">
        <param name="ios-package" value="CDVWKWebViewEngine" />
    </feature>
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
</widget>
